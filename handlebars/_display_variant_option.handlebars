<div class="variant-option">
  <div class="variant-price">
    Price: <span class="price">{{ unit_price }}</span>
  </div>
  {{#each dimensions}}
    <div class="variant-description">
      <span class="variant-name">{{ name }}</span>: <span class="variant-value">{{ value }}</span>
    </div>
  {{/each}}
  {{#if quantity}}
    <div class="variant-quantity">
      Quantity: <span class="quantity">{{ quantity }}</span>
    </div>
  {{/if}}
</div>
